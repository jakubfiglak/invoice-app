datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model User {
  id             String  @id @default(cuid())
  email          String  @unique
  name           String?
  avatarUrl      String?
  hashedPassword String
  salt           String
  resetToken     String?
}
